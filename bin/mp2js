#!/usr/bin/env python
import sys
import msgpack
import ujson


def mp2js(f):
    for r in msgpack.Unpacker(f):
        print ujson.dumps(r)


def main(argv):
    files = argv[1:] if len(argv)>1 else ['-']

    for filename in files:
        if filename=='-':
            mp2js(sys.stdin)
        else:
            with open(filename) as f:
                mp2js(f)


if __name__ == '__main__':
    main(sys.argv)
